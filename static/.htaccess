<IfModule mod_rewrite.c>
  ErrorDocument 404 /404.html
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]

  RewriteRule \.html$ - [L]

  RewriteRule static/img/(.*)$ /img/$1 [L]

  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
