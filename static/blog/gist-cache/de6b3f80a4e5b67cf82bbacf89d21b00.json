g, '\"')\n                .replace(/'/g, ''');\n        },\n        formatGistLinesAsHtml(lines)\n        {\n            let result = '';\n            if (lines && lines.length>0)\n            {\n                let linesArray = lines.split('\\n');\n                for (let line in linesArray)\n                {\n                    result += (this.highlightedLine==parseInt(line)+1 ?\n                        '<tr class=\"highlighted-line\">\\n' :\n                        '<tr>\\n') +\n                    '<td class=\"blob-num js-line-number\" data-line-number=\"' +\n                    (parseInt(line)+1).toString() + '\"></td>\\n' +\n                    '<td class=\"blob-code blob-code-inner js-file-line\">' +\n                    this.escapeHtml(linesArray[line]) +\n                    '</td>\\n' +\n                    '</tr>\\n';\n                }\n            }\n            return result;\n        }\n    },\n};\n</script>\n\n<style lang=\"sass\">\n@import '~/assets/sass/gist.sass'\n</style>\n\n\n"},"extra-component.md":{"filename":"extra-component.md","type":"text/markdown","language":"Markdown","raw_url":"https://gist.githubusercontent.com/Strahinja/de6b3f80a4e5b67cf82bbacf89d21b00/raw/6c9395f1975a49b5141eade104bbd8f2cc5e2524/extra-component.md","size":128,"truncated":false,"content":"<component :is=\"extraComponentLoader\"\n    :filename=\"'nuxt.config.js'\"\n    :highlighted-line=\"15\"\n    :gist=\"gist\"></component>\n"}},"public":true,"created_at":"2019-11-13T09:38:28Z","updated_at":"2019-11-19T13:59:32Z","description":"Vue components in a Markdown file","comments":0,"user":null,"comments_url":"https://api.github.com/gists/de6b3f80a4e5b67cf82bbacf89d21b00/comments","owner":{"login":"Strahinja","id":981077,"node_id":"MDQ6VXNlcjk4MTA3Nw==","avatar_url":"https://avatars3.githubusercontent.com/u/981077?v=4","gravatar_id":"","url":"https://api.github.com/users/Strahinja","html_url":"https://github.com/Strahinja","followers_url":"https://api.github.com/users/Str